{
	"name": "pipeline",
	"properties": {
		"activities": [
			{
				"name": "GenerateParquet",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzurePostgreSqlSource",
						"query": "Select *\nFrom public.person",
						"partitionOption": "None"
					},
					"sink": {
						"type": "ParquetSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "ParquetWriteSettings"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "person_id",
									"type": "Int32",
									"physicalType": "integer"
								},
								"sink": {
									"name": "person_id",
									"type": "Int32",
									"physicalType": "INT_32"
								}
							},
							{
								"source": {
									"name": "first_name",
									"type": "String",
									"physicalType": "character varying"
								},
								"sink": {
									"name": "first_name",
									"type": "String",
									"physicalType": "UTF8"
								}
							},
							{
								"source": {
									"name": "last_name",
									"type": "String",
									"physicalType": "character varying"
								},
								"sink": {
									"name": "last_name",
									"type": "String",
									"physicalType": "UTF8"
								}
							},
							{
								"source": {
									"name": "age",
									"type": "Int32",
									"physicalType": "integer"
								},
								"sink": {
									"name": "age",
									"type": "Int32",
									"physicalType": "INT_32"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "Person",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "PersonParquet",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}