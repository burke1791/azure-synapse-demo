{
	"name": "CreateParquet",
	"properties": {
		"activities": [
			{
				"name": "CreateParquet",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlDWSource",
						"sqlReaderQuery": {
							"value": "@pipeline().parameters.query",
							"type": "Expression"
						},
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "ParquetSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "ParquetWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "ServerlessSQL",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "UTCDateParquet",
						"type": "DatasetReference"
					}
				]
			}
		],
		"parameters": {
			"query": {
				"type": "string",
				"defaultValue": "Select [UTCDate] = '10 Oct 2022'"
			}
		},
		"annotations": []
	}
}